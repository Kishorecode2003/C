#include <stdio.h>
#include<string.h>
int main() {
    char str[50],str1[50];
    printf("Enter the String:");
    scanf("%s",&str);
    printf("Enter the String:");
    scanf("%s",&str1);
    int n=strlen(str);
    int m=strlen(str1);
    printf("%s\n",strcat(str,str1));
    printf("%s\n",strcpy(str,str1));
    //printf("%d",strcmp(str,str1));
    return 0;
}
