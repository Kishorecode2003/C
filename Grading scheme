#include<stdio.h>
int main()
{
    int n;
    printf("Enter the mark: ");
    scanf("%d",&n);
    if(n>=90 && n<=100)
    {
        printf("A+");
    }
    else if(n>=80 && n<90)
    {
        printf("A");
    }
    else if(n>=70 && n<80)
    {
        printf("B");
    }
    else if(n>=60 && n<70)
    {
        printf("C");
    }
    else if(n>=50 && n<60)
    {
        printf("D");
    }
    else if(n<50)
    {
        printf("F");
    }
    else
    {
        printf("Invalid mark");
    }
    return 0;
}
